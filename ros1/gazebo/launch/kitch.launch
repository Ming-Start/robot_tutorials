<?xml version="1.0"?>
<launch>
  <!-- Simulation with UR5e robot arm -->
  <include file="$(find ur_gazebo)/launch/ur5e_bringup.launch">
    <arg name="robot_description_file" value="$(find fg_gazebo_example)/launch/include/load_robot.launch" />
  </include>

  <!-- Moveit motion planning & execution -->
  <include file="$(find ur5e_moveit_config)/launch/moveit_planning_execution.launch">
    <arg name="sim" value="true" />
  </include>

  <!-- Spawn the table model from the Gazebo model library -->
  <node name="spawn_table" pkg="gazebo_ros" type="spawn_model"
        args="-model table -sdf -f //usr/share/gazebo-11/models/table/model.sdf -x 0 -y 1 -z -0.75" />
  <!-- Spawn the bowl model from the Gazebo model library -->
  <node name="spawn_bowl_0" pkg="gazebo_ros" type="spawn_model"
        args="-model bowl_0 -sdf -f /usr/share/gazebo-11/models/Footed_Bowl_Sand/model.sdf -x -0.3 -y 0.7 -z 0.27" />
  <node name="spawn_bowl_1" pkg="gazebo_ros" type="spawn_model"
        args="-model bowl_1 -sdf -f /usr/share/gazebo-11/models/Footed_Bowl_Sand/model.sdf -x 0.3 -y 0.7 -z 0.27" />
  <!-- Spawn the beer model from the Gazebo model library -->
  <node name="spawn_bottle" pkg="gazebo_ros" type="spawn_model"
        args="-model bottle -sdf -f /usr/share/gazebo-11/models/Weston_No_22_Cajun_Jerky_Tonic_12_fl_oz_nLj64ZnGwDh/model.sdf -x 0 -y 0.7 -z 0.27" />

  <!-- Foxglove bridge -->
  <include file="$(find foxglove_bridge)/launch/foxglove_bridge.launch" />
</launch>